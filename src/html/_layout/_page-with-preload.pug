block vars

doctype html
html(lang="ja")
  head
    include ./_head.pug
  body
    .l-page.js-pjax-page(data-page-id=page.id)
      .p-contents-overlay.js-pjax-overlay.is-expand
      .p-progress.js-pjax-progress
        .p-progress__marker
      .l-dummy-scroll.js-dummy-scroll
      .l-contents.js-contents
        .js-pjax-contents
          block content
    block script
    script(src="/pjax/js/main_with_preload.js" async)
